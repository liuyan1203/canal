package com.haiziwang.pmc.model.po;

import com.haiziwang.pmc.constant.ErrorEnum;

import java.io.Serializable;

public class Resp implements Serializable {
    private Long errno;
    private String errmsg = new String();

    public Resp() {
        this.errno = ErrorEnum.SUCCESS.getErrno();
        this.errmsg = ErrorEnum.SUCCESS.getErrmsg();
    }

    public Resp(ErrorEnum em) {
        this.errno = em.getErrno();
        this.errmsg = em.getErrmsg();
    }

    public Resp(Long resultCode, String msg) {
        this.errno = resultCode;
        this.errmsg = msg;
    }

    public void setErrorEnum(ErrorEnum em) {
        this.errno = em.getErrno();
        this.errmsg = em.getErrmsg();
    }

    public Long getErrno() {
        return errno;
    }

    public void setErrno(Long errno) {
        this.errno = errno;
    }

    public String getErrmsg() {
        return errmsg;
    }

    public void setErrmsg(String errmsg) {
        this.errmsg = errmsg;
    }
}
