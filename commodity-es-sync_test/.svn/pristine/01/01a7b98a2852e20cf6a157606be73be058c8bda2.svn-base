package com.haiziwang.pmc.constant;


public enum ErrorEnum {
    SUCCESS(0L, "成功"),
    ERROR_PARAM_ERROR(1023L, "参数错误"),
    ERROR_CONFIG_ERROR(1024L, "系统参数配置错误"),
    ERROR_QUERY_ES_HTTP(1025L, "ES-HTTP查询失败"),
    ERROR_QUERY_ES_FAILED(1026L, "ES数据查询失败"),

    ERROR_NO_RESULT(2000L, "[异常失败]服务执行异常，请通过日志排查"), // 尽量废弃不要用通用返回错误

    ERROR_PARAM_INVALID(2001L, "参数校验失败"),

    ERROR_LOGOUT(3003L,"请登录");

    private Long errno;
    private String errmsg;

    ErrorEnum(Long errno, String errmsg) {
        this.errno = errno;
        this.errmsg = errmsg;
    }

    public Long getErrno() {
        return errno;
    }

    public void setErrno(Long errno) {
        this.errno = errno;
    }

    public String getErrmsg() {
        return errmsg;
    }

    public void setErrmsg(String errmsg) {
        this.errmsg = errmsg;
    }

    @Override
    public String toString() {
        return "code[" + this.errno + "] msg:" + this.errmsg;
    }
}
